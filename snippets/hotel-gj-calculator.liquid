          <div>
            <label class="checkbox-label-gj-calc" id="checkbox_connection2" for="connection2">
              {{- 'gj_calculator.allgemein.regendusche' | t -}}
            </label>
          </div>
        </div>
        <input
          type="checkbox"
          name="connection1"
          id="connection1"
          class="user-input-checkbox user-input-checkbox-all"
          checked
        >
        <input type="checkbox" name="connection2" id="connection2" class="user-input-checkbox user-input-checkbox-all">
      </div>
    </div>

