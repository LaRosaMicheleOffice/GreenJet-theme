<style>
  /*============= page loader css start ========*/
  .yv-page-loader {
    position: fixed;
    width: 100%;
    height: 100%;
    background-color: rgba(255, 255, 255, 1);
    left: 0;
    top: 0;
    display: flex;
    align-items: center;
    justify-content: center;
    z-index:99;
    opacity:0;
    transform: translateY(-100%);
  }
  .page-loading .yv-page-loader{
    opacity:1;
    transform: translateY(0);
  }
  .yv-page-loader img{
    max-width: 100px;
  }
  /*============= page loader css end ========*/
  .wow {
    visibility: hidden;
    animation-name: none;
  }
  /* syncopate-regular - latin */
@font-face {
  font-family: 'Syncopate';
  font-style: normal;
  font-weight: 400;
  src: url('https://cdn.shopify.com/s/files/1/0633/5778/0153/files/syncopate-regular.eot'); /* IE9 Compat Modes */
  src: local(''),
       url('https://cdn.shopify.com/s/files/1/0633/5778/0153/files/syncopate-regular.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
       url('https://cdn.shopify.com/s/files/1/0633/5778/0153/files/syncopate-regular.woff2') format('woff2'), /* Super Modern Browsers */
       url('https://cdn.shopify.com/s/files/1/0633/5778/0153/files/syncopate-regular.woff') format('woff'), /* Modern Browsers */
       url('https://cdn.shopify.com/s/files/1/0633/5778/0153/files/syncopate-regular.ttf') format('truetype'), /* Safari, Android, iOS */
       url('https://cdn.shopify.com/s/files/1/0633/5778/0153/files/syncopate-regular.svg#Syncopate') format('svg'); /* Legacy iOS */
}
</style>

