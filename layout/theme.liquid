<style>
  * {
    transition: none !important;
    animation: none !important;
  }
</style>

  
  <body class="template_{{ template | handle }} page-loading breadcrumb-{{ settings.enable_breadcrumb }}">   
    <a class="skip-to-content-link button yv-visually-hidden focus-inside" href="#MainContent">
      {{ "accessibility.skip_to_text" | t }}
    </a>
    <!-- Google Tag Manager (noscript) -->
    <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-KN6DZ84"
    height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
    <!-- End Google Tag Manager (noscript) -->
    <div class="body-wrapper">
      <div class="wrapper"> 
        <div id="headerSection">
          {%- section 'announcement-bar' -%}
          {%- section 'header' -%} 
        </div>
        {%- if settings.enable_breadcrumb -%}
        {%- render 'breadcrumb' -%}      
        {%- endif -%}
        <main id="MainContent" class="content-for-layout focus-none">
          {{ content_for_layout }} 
        </main>      
        {%- section 'footer' -%}  
        {%- render 'search-drawer' -%}     
        {%- render 'side-drawer' -%}
        <ul class="list-unstyled" hidden>
          <li id="a11y-refresh-page-message">{{ 'accessibility.refresh_page' | t }}</li>
        </ul>
      </div>  
      {%- liquid 
      	section 'offer-sidebar'
    	render 'newsletter-popup'
        if settings.enableScrollToTop 
        	render 'scroll-top'
        endif 
       render 'similar-product-drawer'
       -%}
    </div>
  
<!-- **BEGIN** Hextom CTB Integration // Main Include - DO NOT MODIFY -->
    {% render 'hextom_ctb_main', product: product, customer: customer %}
    <!-- **END** Hextom CTB Integration // Main Include - DO NOT MODIFY -->
    {% if request.host == 'greenjet.shop' %}
    {% render 'whatsapp' %}
    {% endif %}
    
{% render "bc_banner" %}

<script src="{{ 'jquery.min.js' | asset_url }}"></script> 

  </body>
</html>